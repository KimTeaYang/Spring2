<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">


	<bean id="sv1" class="ex04.ServiceImpl">
		<property name="list">
			<list>
				<value>Java</value>
				<value>Html</value>
				<value>Spring</value>
			</list>	
		</property>
	</bean>
	
	<bean id="sv2" class="ex04.ServiceImpl">
		<property name="map">
			<map>
				<entry key="age" value="28" />
				<entry key="salary" value="3000" />
			</map>
		</property>
	</bean>
	
	<bean id="sv3" class="ex04.ServiceImpl">
		<property name="set">
			<set>
				<value>Oracle</value>
				<value>MySql</value>
				<value>MsSQL</value>
			</set>
		</property>
	</bean>
	
	<bean id="sv4" class="ex04.ServiceImpl">
		<property name="prop">
			<props>
				<prop key="user">192.168.0.94</prop>
				<prop key="pwd">5050</prop>
			</props>
		</property>
	</bean>

</beans>
